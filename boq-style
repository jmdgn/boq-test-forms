    <script type="text/javascript">
       // Character count in text box
       const textBox = document.querySelector("textarea");
         const charCount = document.getElementById("charCount");

         if (textBox) {
             textBox.addEventListener("input", () => {
                 const currentLength = textBox.value.length;
                 const maxLength = textBox.maxLength;
                 charCount.textContent = `${currentLength}/${maxLength}`;
             });
         }
       // End


       // Custom Error Message
       const form = document.getElementById('marketingForm');
       const inputs = form.querySelectorAll('input');

       inputs.forEach(input => {
           const eventType = input.type === 'radio' ? 'change' : 'blur';
           input.addEventListener(eventType, () => {
               validateInput(input);
               updateParentStyling(input);
           });
       });

       function validateInput(input) {
           if (input.type === 'radio') {
               const radioGroup = form.querySelectorAll(`input[name="${input.name}"]`);
               const isChecked = Array.from(radioGroup).some(radio => radio.checked);

               radioGroup.forEach(radio => {
                   if (!isChecked) {
                       radio.classList.add('invalid');
                   } else {
                       radio.classList.remove('invalid');
                   }
               });
           } else {
               if (!input.validity.valid) {
                   input.classList.add('invalid');
               } else {
                   input.classList.remove('invalid');
               }
           }
       }

       function updateParentStyling(input) {
           const parentBlock = input.closest('.phoneFormFieldBlock');
           if (parentBlock) {
               if (input.classList.contains('invalid')) {
                   parentBlock.classList.add('invalid');
               } else {
                   parentBlock.classList.remove('invalid');
               }
           }
       }

       form.addEventListener('submit', function(e) {
           let isValid = true;

           inputs.forEach(input => {
               validateInput(input);
               updateParentStyling(input);

               if (input.classList.contains('invalid')) {
                   isValid = false;
               }
           });

           if (!isValid) {
               e.preventDefault();
           }
       });
       // End

       // Error Message on Dropdown and Text Area Options
       document.addEventListener('DOMContentLoaded', function() {
           const selectElements = document.querySelectorAll('select');
           const textareaElements = document.querySelectorAll('textarea');

           function validateSelects() {
               selectElements.forEach(selectElement => {
                   if (selectElement.value === "") {
                       selectElement.classList.add('invalid');
                   } else {
                       selectElement.classList.remove('invalid');
                   }
               });
           }

           function validateTextareas() {
               textareaElements.forEach(textareaElement => {
                   if (textareaElement.value.trim() === "") {
                       textareaElement.classList.add('invalid');
                   } else {
                       textareaElement.classList.remove('invalid');
                   }
               });
           }

           document.querySelector('form').addEventListener('submit', function(event) {
               validateSelects();
               validateTextareas();

               const selectsValid = Array.from(selectElements).every(selectElement => selectElement.value !== "");
               const textareasValid = Array.from(textareaElements).every(textareaElement => textareaElement.value.trim() !== "");

               if (!selectsValid || !textareasValid) {
                   event.preventDefault();
               }
           });

           selectElements.forEach(selectElement => {
               selectElement.addEventListener('change', validateSelects);
           });

           textareaElements.forEach(textareaElement => {
               textareaElement.addEventListener('input', validateTextareas);
           });
       });
       // End

       // Error Message for Radio Buttons
       function checkInvalidInputs() {
           const radiobuttonsDivs = document.querySelectorAll('.radiobuttons');

           radiobuttonsDivs.forEach((radiobuttonsDiv) => {
               const hasInvalidInput = Array.from(radiobuttonsDiv.querySelectorAll('input'))
                   .some(input => input.classList.contains('invalid'));

               if (hasInvalidInput) {
                   radiobuttonsDiv.classList.add('error');
               } else {
                   radiobuttonsDiv.classList.remove('error');
               }
           });
       }

       const observer = new MutationObserver((mutationsList) => {
           mutationsList.forEach((mutation) => {
               if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
                   checkInvalidInputs();
               }
           });
       });

       document.querySelectorAll('.radiobuttons input').forEach((input) => {
           observer.observe(input, {
               attributes: true
           });
       });

       checkInvalidInputs();
       // End

       // Error Message for Drop Down Select
       document.addEventListener("DOMContentLoaded", function() {
           const form = document.querySelector("form");

           form.addEventListener("submit", function(event) {
               const optionBlocks = document.querySelectorAll(".optionSetFormFieldBlock");
               let hasError = false;

               optionBlocks.forEach(block => block.classList.remove("error"));

               optionBlocks.forEach((block) => {
                   const selectElement = block.querySelector("select");

                   if (selectElement && !selectElement.checkValidity()) {
                       selectElement.classList.add("invalid");
                       block.classList.add("error");
                       hasError = true;
                   } else {
                       selectElement.classList.remove("invalid");
                   }
               });

               if (hasError) {
                   event.preventDefault();
               }
           });

           const optionBlocks = document.querySelectorAll(".optionSetFormFieldBlock");
           optionBlocks.forEach(block => {
               const selectElement = block.querySelector("select");

               if (selectElement) {
                   selectElement.addEventListener("change", function() {
                       if (selectElement.classList.contains("invalid")) {
                           selectElement.classList.remove("invalid");
                           block.classList.remove("error");
                       }
                   });
               }
           });
       });
       // End

       // Additional spacing for error messages
       function checkOuterForInvalid() {
           const outerDivs = document.querySelectorAll('.outer');

           outerDivs.forEach((outerDiv) => {
               const hasInvalidElement = outerDiv.querySelector('.invalid') !== null;

               if (hasInvalidElement) {
                   outerDiv.classList.add('error-space');
               } else {
                   outerDiv.classList.remove('error-space');
               }
           });
       }

       const outerObserver = new MutationObserver((mutationsList) => {
           mutationsList.forEach((mutation) => {
               if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
                   checkOuterForInvalid();
               }
           });
       });

       document.querySelectorAll('.outer *').forEach((element) => {
           outerObserver.observe(element, {
               attributes: true
           });
       });

       checkOuterForInvalid();
       //End

       // Select Drop Down Error Message Script
       document.querySelector('form').addEventListener('submit', function(event) {
           const selectElement = document.querySelector('select[name="msdyn_salesassignmentresult"]');
           const customSelectDiv = document.querySelector('.custom-select');

           if (selectElement.classList.contains('invalid')) {
               customSelectDiv.classList.add('error');
           } else {
               customSelectDiv.classList.remove('error');
           }
       });
       // End

       // Select Dropdown Custom Styles
       var x, i, j, l, ll, selElmnt, a, b, c;
       x = document.getElementsByClassName("custom-select");
       l = x.length;
       for (i = 0; i < l; i++) {
           selElmnt = x[i].getElementsByTagName("select")[0];
           ll = selElmnt.length;
           a = document.createElement("DIV");
           a.setAttribute("class", "select-selected");
           a.innerHTML = selElmnt.options[selElmnt.selectedIndex].innerHTML;
           x[i].appendChild(a);
           b = document.createElement("DIV");
           b.setAttribute("class", "select-items select-hide");
           for (j = 1; j < ll; j++) {
               c = document.createElement("DIV");
               c.innerHTML = selElmnt.options[j].innerHTML;
               c.addEventListener("click", function(e) {
                   var y, i, k, s, h, sl, yl;
                   s = this.parentNode.parentNode.getElementsByTagName("select")[0];
                   sl = s.length;
                   h = this.parentNode.previousSibling;
                   for (i = 0; i < sl; i++) {
                       if (s.options[i].innerHTML == this.innerHTML) {
                           s.selectedIndex = i;
                           h.innerHTML = this.innerHTML;

                           h.classList.add("has-selection");

                           y = this.parentNode.getElementsByClassName("same-as-selected");
                           yl = y.length;
                           for (k = 0; k < yl; k++) {
                               y[k].removeAttribute("class");
                           }
                           this.setAttribute("class", "same-as-selected");
                           break;
                       }
                   }
                   h.click();
               });
               b.appendChild(c);
           }
           x[i].appendChild(b);
           a.addEventListener("click", function(e) {
               e.stopPropagation();
               closeAllSelect(this);
               this.nextSibling.classList.toggle("select-hide");
               this.classList.toggle("select-arrow-active");
           });
       }

       function closeAllSelect(elmnt) {
           var x, y, i, xl, yl, arrNo = [];
           x = document.getElementsByClassName("select-items");
           y = document.getElementsByClassName("select-selected");
           xl = x.length;
           yl = y.length;
           for (i = 0; i < yl; i++) {
               if (elmnt == y[i]) {
                   arrNo.push(i);
               } else {
                   y[i].classList.remove("select-arrow-active");
               }
           }
           for (i = 0; i < xl; i++) {
               if (arrNo.indexOf(i)) {
                   x[i].classList.add("select-hide");
               }
           }
       }
       document.addEventListener("click", closeAllSelect);
       // End

       // Date Calendar Scripts
       $(function() {
           $(".datepicker").datepicker({
               dateFormat: "dd-mm-yy",
               duration: "fast",
               showButtonPanel: true,
               currentText: "Today",
               beforeShow: function(input, inst) {
                   setTimeout(function() {
                       var buttonPanel = $(inst.dpDiv).find(".ui-datepicker-buttonpane");

                       buttonPanel.find("button.ui-datepicker-close").remove();

                       if (buttonPanel.find(".ui-datepicker-clear").length === 0) {
                           $("<button>", {
                               text: "Clear",
                               class: "ui-datepicker-clear ui-state-default ui-priority-primary ui-corner-all",
                               click: function() {
                                   $(input).val("").removeClass("has-date");
                                   $(".datepicker").datepicker("hide");
                               }
                           }).prependTo(buttonPanel);
                       }

                       var todayButton = buttonPanel.find("button.ui-datepicker-current");
                       if (todayButton.length) {
                           todayButton.off("click").on("click", function() {
                               var today = $.datepicker.formatDate("dd-mm-yy", new Date());
                               $(input).val(today).addClass("has-date");
                               $(".datepicker").datepicker("hide");
                           });
                           todayButton.appendTo(buttonPanel);
                       }
                   }, 1);
               },
               onClose: function() {
                   $(".ui-datepicker-clear").remove();
               },
               onSelect: function(dateText, inst) {
                   if (dateText) {
                       $(this).addClass("has-date");
                   } else {
                       $(this).removeClass("has-date");
                   }
               }
           });
       });
       // End

       // Tel Input Confirmation Styles
       document.addEventListener("DOMContentLoaded", () => {
           // Select the input inside the .phoneCountryCode div
           const input = document.querySelector(".phoneCountryCode input");
           const parentDiv = input.closest(".phoneCountryCode");

           input.addEventListener("input", () => {
               if (input.checkValidity()) {
                   parentDiv.classList.add("valid");
                   parentDiv.classList.remove("invalid");
               } else {
                   parentDiv.classList.add("invalid");
                   parentDiv.classList.remove("valid");
               }
           });
       });
       // End
      </script>

  </body>
</html>
